pipeline{
	agent any
		stages{
			stage('build'){
				steps{
					echo "build stage"
					}
				}
			stage('test'){
				steps{
					echo "test stage enabling polling"
					bat 'set'
					}
				}
			stage('deploy'){
				steps{
					echo "deploy stage"
					echo "current build number is ${env.BUILD_NUMBER}"
					}
				}
			}
		
	}
			