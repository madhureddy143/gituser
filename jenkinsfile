pipeline{
	agent any
	parameters {
		string (name: 'name', defaultvalue: 'madhu',description: 'name of the user')
		}
		stages{
			stage('build'){
				steps{
					echo "build stage"
					}
				}
			stage('test'){
				steps{
					echo "test stage enabling polling"
					bat 'set'
					}
				}
			stage('deploy'){
				steps{
					echo "deploy stage"
					echo "current build number is ${env.BUILD_NUMBER}"
					}
				}
			}
		
	}
			